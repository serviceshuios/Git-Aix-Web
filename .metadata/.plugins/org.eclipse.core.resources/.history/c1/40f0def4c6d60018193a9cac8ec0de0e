/**
 * CREATION DES OBJETS
 */

var personne1 = new Object();
	personne1.nom = "toto";
	personne1.prenom = "tata";
	personne1.age = 25;
	
	
alert(personne1.nom);


var personne2 = {
		prenom : "union",
		nom:"zec",
		age:19,
		id : function() {
			return this.prenom + ' '+ this.nom+ ' '+this.age;
		}
}

alert(personne2.id);